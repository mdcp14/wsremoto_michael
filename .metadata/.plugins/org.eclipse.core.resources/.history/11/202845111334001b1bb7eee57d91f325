package repeticao;

import javax.swing.JOptionPane;

public class DesafioDo {

	public static void main(String[] args) {
		
		int num1 = 0;
		int num2 = 0;
		
		do {
			num1 = Integer.parseInteger(JOptionPane.showInputDialog("Digite um número"));
		
		}while 
			(num1 == num2); 
			System.out.println("Número não é igual");
		
	 
	
	
	
	
	
	
	
	
	}

}
