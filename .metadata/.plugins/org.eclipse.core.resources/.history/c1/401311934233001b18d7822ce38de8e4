package variaveis;

public class ManipulaString {

	public static void main(String[] args) {
		
		String email = "xPtO@gaMa.com";
		
		System.out.print("Original: "   + email);
		System.out.print("Maiuscula: " + email.toUpperCase());
		System.out.print("Minusculo: " + email.toLowerCase());
		
		
		
		
		

	}

}
