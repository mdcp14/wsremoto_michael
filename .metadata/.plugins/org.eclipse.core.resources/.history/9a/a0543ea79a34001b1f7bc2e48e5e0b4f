package repeticao;

public class ExemploFor {

	public static void main(String[] args) {
		/*For = próprio para quando não existe interação com end user
		Sintaxe:
		1 - definir o contador;
		2 - qual a condição para manter a repeticação
		3 - qual a forma de contagem
		*/
		
		for (int cont=1;cont<500; cont+=2) {
			System.out.println(cont);
		}
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		

	}

}
